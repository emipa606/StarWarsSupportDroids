<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.ThingDef_AlienRace ParentName="SW_Droid_Base">
    <defName>GNK_Power</defName>
    <label>GNK Power Droid</label>
    <description>The GNK-series power droids, also known as gonk droids, were an Industrial Automaton knockoff of the successful EG-6 power droid.</description>
    <alienRace>
      <generalSettings>
        <forcedRaceTraitEntries>
          <li>
            <defName>SupportAI</defName>
            <chance>100</chance>
          </li>
        </forcedRaceTraitEntries>
        <notXenophobistTowards>
          <li>Human</li>
          <!--<li>21B_Medical</li>-->
          <li>FX7_MedicalAssistant</li>
          <li>GNK_Power</li>
          <li>MSE6_Repair</li>
          <li>Viper_Probe</li>
        </notXenophobistTowards>
        <alienPartGenerator>
          <aliencrowntypes>
            <li>Male</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Male</li>
          </alienbodytypes>
          <useGenderedHeads>false</useGenderedHeads>
          <useGenderedBodies>false</useGenderedBodies>
          <customDrawSize>(0.7,0.7)</customDrawSize>
          <customPortraitDrawSize>(0.75,0.75)</customPortraitDrawSize>
          <colorChannels>
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <li>
                    <!--Dirt-->
                    <weight>75</weight>
                    <only>(125,90,35)</only>
                  </li>
                  <li>
                    <!--Grey-->
                    <weight>25</weight>
                    <only>(120,120,120)</only>
                  </li>
                </options>
              </first>
            </li>
          </colorChannels>
        </alienPartGenerator>
      </generalSettings>
      <graphicPaths>
        <li>
          <body>Races/GNK/</body>
        </li>
      </graphicPaths>
      <raceRestriction>
        <onlyUseRaceRestrictedWeapons>true</onlyUseRaceRestrictedWeapons>
      </raceRestriction>
    </alienRace>
    <statBases>
      <MarketValue>100</MarketValue>
      <MedicalTendQuality>0</MedicalTendQuality>
      <MedicalSurgerySuccessChance>0</MedicalSurgerySuccessChance>
      <GeneralLaborSpeed>0</GeneralLaborSpeed>
      <MeleeDodgeChance>.75</MeleeDodgeChance>
      <ToxicSensitivity>0</ToxicSensitivity>
      <MoveSpeed>2.00</MoveSpeed>
      <Flammability>0</Flammability>
      <ImmunityGainSpeed>10</ImmunityGainSpeed>
      <ArmorRating_Sharp>.40</ArmorRating_Sharp>
      <ArmorRating_Blunt>.20</ArmorRating_Blunt>
      <ArmorRating_Heat>2.00</ArmorRating_Heat>
      <ComfyTemperatureMin>-200</ComfyTemperatureMin>
      <ComfyTemperatureMax>200</ComfyTemperatureMax>
      <PainShockThreshold>0.99</PainShockThreshold>
      <HungerRateMultiplier>0</HungerRateMultiplier>
      <CarryingCapacity>0</CarryingCapacity>
      <Mass>30</Mass>
      <EatingSpeed>0</EatingSpeed>
      <WorkSpeedGlobal>0</WorkSpeedGlobal>
      <LeatherAmount>0</LeatherAmount>
      <MeatAmount>0</MeatAmount>
      <ResearchSpeed>0</ResearchSpeed>
      <PlantWorkSpeed>0</PlantWorkSpeed>
      <GlobalLearningFactor>0</GlobalLearningFactor>
      <!--violence -->
      <HuntingStealth>0</HuntingStealth>
      <NegotiationAbility>0</NegotiationAbility>
      <TradePriceImprovement>0</TradePriceImprovement>
      <SocialImpact>0</SocialImpact>
