<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.ThingDef_AlienRace ParentName="SW_Droid_Base">
    <defName>21B_Medical</defName>
    <label>2-1B Surgical Droid</label>
    <description>The 2-1B-series surgical droid were advanced medical droids produced by Industrial Automaton.</description>
    <alienRace>
      <generalSettings>
        <forcedRaceTraitEntries>
          <li>
            <defName>SupportAI</defName>
            <chance>100</chance>
          </li>
        </forcedRaceTraitEntries>
        <notXenophobistTowards>
          <li>Human</li>
          <!--<li>21B_Medical</li>-->
          <li>FX7_MedicalAssistant</li>
          <li>GNK_Power</li>
          <li>MSE6_Repair</li>
          <li>Viper_Probe</li>
        </notXenophobistTowards>
        <alienPartGenerator>
          <aliencrowntypes>
            <li>Male</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Male</li>
          </alienbodytypes>
          <useGenderedHeads>false</useGenderedHeads>
          <useGenderedBodies>false</useGenderedBodies>
          <customDrawSize>(1,1)</customDrawSize>
          <customHeadDrawSize>(1.35,1.35)</customHeadDrawSize>
          <customPortraitDrawSize>(1,1)</customPortraitDrawSize>
          <customPortraitHeadDrawSize>(1.35,1.35)</customPortraitHeadDrawSize>
          <headOffset>(0,0)</headOffset>
          <bodyAddons>
            <li>
              <drawSize>1.25</drawSize>
              <path>Races/21B/Head/Male</path>
              <bodyPart>SW_Medical_Head</bodyPart>
              <inFrontOfBody>true</inFrontOfBody>
              <colorChannel>skin</colorChannel>
              <defaultOffset>Tail</defaultOffset>
              <offsets>
                <south>
                  <layerOffset>0.02</layerOffset>
                  <bodyTypes>
                    <Male>(-0.42, 0.178)</Male>
                  </bodyTypes>
                </south>
                <north>
                  <layerOffset>0.02</layerOffset>
                  <bodyTypes>
                    <Male>(0, 0.513)</Male>
                  </bodyTypes>
                </north>
                <east>
                  <layerOffset>0.02</layerOffset>
                  <bodyTypes>
                    <Male>(-0.43, 0.165)</Male>
                  </bodyTypes>
                  <crownTypes>
                    <Male>(0, 0)</Male>
                  </crownTypes>
                </east>
              </offsets>
            </li>
            <li>
              <drawSize>1.25</drawSize>
              <path>Races/21B/Head2/Male</path>
              <bodyPart>SW_Medical_Head</bodyPart>
              <inFrontOfBody>false</inFrontOfBody>
              <colorChannel>skin</colorChannel>
              <defaultOffset>Tail</defaultOffset>
              <offsets>
                <south>
                  <layerOffset>0.02</layerOffset>
                  <bodyTypes>
                    <Male>(-0.42, 0.178)</Male>
                  </bodyTypes>
                </south>
                <north>
                  <layerOffset>0.02</layerOffset>
                  <bodyTypes>
                    <Male>(0, 0.513)</Male>
                  </bodyTypes>
                </north>
                <east>
                  <layerOffset>0.02</layerOffset>
                  <bodyTypes>
                    <Male>(-0.43, 0.165)</Male>
                  </bodyTypes>
                  <crownTypes>
                    <Male>(0, 0)</Male>
                  </crownTypes>
                </east>
              </offsets>
            </li>
          </bodyAddons>
          <colorChannels>
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <li>
                    <!--Light Grey-->
                    <weight>50</weight>
                    <only>(140,140,140)</only>
                  </li>
                  <li>
                    <!--Grey-->
                    <weight>50</weight>
                    <only>(120,120,120)</only>
                  </li>
                </options>
              </first>
            </li>
          </colorChannels>
        </alienPartGenerator>
      </generalSettings>
      <graphicPaths>
        <li>
          <body>Races/21B/</body>
          <head>Races/21B/Head/</head>
        </li>
      </graphicPaths>
      <raceRestriction>
        <onlyUseRaceRestrictedWeapons>true</onlyUseRaceRestrictedWeapons>
      </raceRestriction>
    </alienRace>
    <statBases>
